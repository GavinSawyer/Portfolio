{
  "name": "console",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/console/src",
  "prefix": "console-app",
  "targets": {
    "prebuild": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node git-version.js"
      }
    },
    "browser": {
      "dependsOn": ["prebuild"],
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/console/browser",
        "index": "apps/console/src/index.html",
        "main": "apps/console/src/main.ts",
        "polyfills": "apps/console/src/polyfills.ts",
        "tsConfig": "apps/console/tsconfig.app.json",
        "inlineStyleLanguage": "sass",
        "assets": [
          "apps/console/src/assets",
          "apps/console/src/favicon.png",
          "apps/console/src/robots.txt",
          "apps/console/src/sitemap.xml"
        ],
        "styles": ["apps/console/src/styles.sass"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/console/src/environments/environment.ts",
              "with": "apps/console/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": true,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "server": {
      "dependsOn": ["prebuild"],
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/apps/console/server",
        "main": "apps/console/server.ts",
        "tsConfig": "apps/console/tsconfig.server.json",
        "inlineStyleLanguage": "sass"
      },
      "configurations": {
        "production": {
          "outputHashing": "media",
          "fileReplacements": [
            {
              "replace": "apps/console/src/environments/environment.ts",
              "with": "apps/console/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false
        }
      },
      "defaultConfiguration": "production",
      "production": {
        "outputHashing": "media",
        "fileReplacements": [
          {
            "replace": "apps/console/src/environments/environment.ts",
            "with": "apps/console/src/environments/environment.prod.ts"
          }
        ]
      },
      "development": {
        "optimization": false,
        "sourceMap": true,
        "extractLicenses": false
      }
    },
    "build": {
      "dependsOn": ["prebuild", "^browser", "^server"],
      "executor": "@nguniversal/builders:prerender",
      "options": {
        "routesFile": "apps/console/src/app/routes.txt"
      },
      "configurations": {
        "production": {
          "browserTarget": "console:browser:production",
          "serverTarget": "console:server:production"
        },
        "development": {
          "browserTarget": "console:browser:development",
          "serverTarget": "console:server:development"
        }
      },
      "defaultConfiguration": "production"
    },
    "deploy": {
      "dependsOn": ["prebuild", "^browser", "^server"],
      "executor": "@angular/fire:deploy",
      "options": {
        "ssr": "cloud-run",
        "prerender": true,
        "firebaseProject": "portfolio-a59e2",
        "firebaseHostingSite": "portfolio-a59e2-console",
        "functionName": "ssr-console",
        "functionsNodeVersion": "18.15",
        "region": "us-central1",
        "browserTarget": "console:browser:production",
        "serverTarget": "console:server:production",
        "prerenderTarget": "console:build:production",
        "outputPath": "dist/apps/console/run"
      }
    },
    "serve": {
      "dependsOn": ["prebuild"],
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "console:browser:production"
        },
        "development": {
          "browserTarget": "console:browser:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "dependsOn": ["prebuild"],
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "console:browser"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/console/**/*.ts", "apps/console/**/*.html"]
      }
    }
  },
  "tags": []
}
